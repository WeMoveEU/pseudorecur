<?xml version="1.0"?>
<extension key="eu.wemove.pseudorecur" type="module">
  <file>pseudorecur</file>
  <name>Pseudo-frequency for recurring contributions</name>
  <description>Allows to transparently convert the frequency of recurring contributions</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Romain Thouvenin</author>
    <email>romain@wemove.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/WeMoveEU/pseudorecur</url>
    <url desc="Documentation">https://github.com/WeMoveEU/pseudorecur</url>
    <url desc="Support">https://github.com/WeMoveEU/pseudorecur</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2017-07-05</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments>This is a new, in development module. At the moment, only weekly-to-monthly conversions are supported.</comments>
  <civix>
    <namespace>CRM/Pseudorecur</namespace>
  </civix>
</extension>
